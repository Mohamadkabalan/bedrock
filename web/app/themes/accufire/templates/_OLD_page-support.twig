{% extends "base.twig" %}

{% block content %}
  <div class="page content-wrapper">
    <article class="post-type-{{post.post_type}}" id="post-{{post.ID}}">
      <section class="article-content">
        <div class="article-body">
          {{post.content}}
          {% include "blocks/support-header.twig" %}
          {% include "blocks/support-questions.twig" %}
          {% include "blocks/support-downloads.twig" %}
          {% include "blocks/downloads-testimonials.twig" %}
          {% include "blocks/support-documentation.twig" %}
          {%  include "blocks/support-faq.twig" %}
        </div>
      </section>
    </article>
  </div>
{% endblock %}
