{% for item in menu.items %}
  <li class="nav-item {{ item.classes|join(' ') }}">
    <a class="nav-link" href="{{ item.link }}" target="{{ item.target }}">{{ item.title }}</a>
    {% if item.children %}
      <ul class="nav-drop">
        {% for child in item.children %}
          <li class="nav-drop-item">
            <a href="{{ child.link }}">{{ child.title }}</a>
          </li>
        {% endfor %}
      </ul>
    {% endif %}
  </li>
{% endfor %}